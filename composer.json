{
    "name": "tkaratug/laravel-domain-whitelist",
    "description": "A domain wwhitelist checker middleware.",
    "keywords": [
        "tkaratug",
        "laravel",
        "laravel-domain-whitelist"
    ],
    "homepage": "https://github.com/tkaratug/laravel-domain-whitelist",
    "license": "MIT",
    "authors": [
        {
            "name": "Turan KaratuÄŸ",
            "email": "tkaratug@hotmail.com.tr",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "spatie/laravel-package-tools": "^1.4.3",
        "illuminate/contracts": "^8.37"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.9",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Tkaratug\\LaravelDomainWhitelist\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tkaratug\\LaravelDomainWhitelist\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tkaratug\\LaravelDomainWhitelist\\LaravelDomainWhitelistServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
