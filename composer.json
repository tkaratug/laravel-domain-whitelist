{
    "name": "tkaratug/laravel-domain-whitelist",
    "description": "A domain wwhitelist checker middleware.",
    "keywords": [
        "tkaratug",
        "laravel",
        "laravel-domain-whitelist"
    ],
    "homepage": "https://github.com/tkaratug/laravel-domain-whitelist",
    "license": "MIT",
    "authors": [
        {
            "name": "Turan KaratuÄŸ",
            "email": "tkaratug@hotmail.com.tr",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0|^8.1",
        "spatie/laravel-package-tools": "^1.4.3",
        "illuminate/support": "^8.0|^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Tkaratug\\LaravelDomainWhitelist\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tkaratug\\LaravelDomainWhitelist\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tkaratug\\LaravelDomainWhitelist\\LaravelDomainWhitelistServiceProvider"
            ]
        }
    }
}
